Frequencies index<br><br>

<% Crime.all.each do |c| %>
    <% Freq.all.each do |f| %>
    	<% if f.rating == 0 %>
	        <% if f.crime_id == c.id %>
	            <% f.rating = c.f %>
	        <% end %>
        <% end %>
    <% end %>
<% end %>

<%= "#{@freqs.size} Responses" %><br><br>
<% @freqs.each do |g| %>
	<% if (g.rating) %>
		<%= "crime id: " %>
		<%= g.crime_id %>
		<%= "rating: " %>
		<%= g.rating %> 
		<br>
	<% end %>
<% end %>
